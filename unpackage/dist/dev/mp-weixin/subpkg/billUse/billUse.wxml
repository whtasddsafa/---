<view class="billUse"><view class="billUse-top"><view class="billUse-top1">订单编号<label class="_span">{{orderDetail.orderSn}}</label></view><view class="billUse-top2">已支付</view></view><view class="purchase-top"><view class="left-img-box-bottom"><view class="img-box"><image src="{{goodsList[0].goodsLogo}}"></image></view><view class="right-box"><view class="text-box-top"><view class="span1"><label class="_span">{{goodsList[0].goodsName}}</label></view><block wx:if="{{goodsList[0].goodsSn=='VIP'}}"><view class="span2"><uni-dateformat vue-id="a0e7f7ec-1" date="{{orderDetail.payTime}}" format="yyyy/MM/dd" bind:__l="__l"></uni-dateformat>-<uni-dateformat vue-id="a0e7f7ec-2" date="{{orderDetail.vipEndTime}}" format="yyyy/MM/dd" bind:__l="__l"></uni-dateformat></view></block><block wx:else><view class="span2">购买日期:<uni-dateformat vue-id="a0e7f7ec-3" date="{{orderDetail.payTime}}" format="yyyy/MM/dd" bind:__l="__l"></uni-dateformat></view></block><view class="span3"><label class="_span">{{goodsList[0].address}}</label></view></view></view></view></view><view class="bill-QR-code"><view class="bill-QR-code1"><image src="{{QRImg}}"></image></view><view></view></view><block wx:if="{{goodsList[0].goodsSn!=='VIP'}}"><view class="hxm"><uinput bind:input="__e" vue-id="a0e7f7ec-4" label="核销:" type placeholder="请输入核销码" value="{{test}}" data-event-opts="{{[['^input',[['__set_model',['','test','$event',[]]]]]]}}" bind:__l="__l"></uinput><button type="default" size="mini" data-event-opts="{{[['tap',[['hx',['$event']]]]]}}" bindtap="__e">确认核销</button></view></block><view class="admissionTicket"><view class="admissionTicket-span"><label class="_span">票品信息</label></view><view class="ticketType"><view class="ticketType-l">门票类型</view><view class="ticketType-r">{{''+goodsList[0].goodsName+''}}</view></view><view class="ticketType"><view class="ticketType-l">单价</view><view class="ticketType-r">{{'￥'+goodsList[0].marketPrice+''}}</view></view><view class="ticketType"><view class="ticketType-l">数量</view><view class="ticketType-r"><label class="_span">{{orderDetail.count}}</label>张</view></view><view class="ticketType" style="border-bottom:1px solid #d0d0d0;"><view class="ticketType-l">总计</view><view class="ticketType-r">￥1</view></view><view class="actualPayment"><view class="actualPayment-l">实付款</view><view class="actualPayment-r">{{'￥'+orderDetail.totalAmount+''}}</view></view></view></view>