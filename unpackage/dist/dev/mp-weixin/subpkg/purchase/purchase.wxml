<view class="tall"><view class="purchase-tall"><view class="purchase-top"><view class="left-img-box-bottom"><view class="img-box"><image src="{{goods.goodsLogo}}"></image></view><view class="right-box"><view class="text-box-top"><view class="span1"><label class="_span">{{goods.goodsName}}</label></view><view class="span2"><uni-dateformat vue-id="1ce1a22c-1" date="{{goods.effectiveStartTime}}" format="yyyy/MM/dd" bind:__l="__l"></uni-dateformat>-<uni-dateformat vue-id="1ce1a22c-2" date="{{goods.effectiveEndTime}}" format="yyyy/MM/dd" bind:__l="__l"></uni-dateformat></view><view class="span3"><label class="_span">{{goods.address}}</label></view></view><view class="text-box-bottom"><view class="text-box-bottom-left"><label class="_span">{{"￥"+goods.marketPrice}}</label></view><block wx:if="{{goodsSn!=='VIP'}}"><view class="top-card-text-right"><label class="_span">{{"VIP票价:￥"+goods.costPrice}}</label></view></block></view></view></view></view><view><block wx:if="{{goodsSn!=='VIP'}}"><view class="CC"><view class="CC1">您的票数:</view><view class="CC2"><uni-number-box bind:input="__e" vue-id="1ce1a22c-3" min="{{1}}" background="black" color="white" value="{{shuliang}}" data-event-opts="{{[['^input',[['__set_model',['','shuliang','$event',[]]]]]]}}" bind:__l="__l"></uni-number-box></view></view></block><view><uinput bind:input="__e" vue-id="1ce1a22c-4" label="您的姓名:" type placeholder="请输入姓名" value="{{test}}" data-event-opts="{{[['^input',[['__set_model',['','test','$event',[]]]]]]}}" bind:__l="__l"></uinput></view><view><uinput bind:input="__e" vue-id="1ce1a22c-5" label="手机号码:" type="tel" placeholder="请输入手机号" value="{{tel}}" data-event-opts="{{[['^input',[['__set_model',['','tel','$event',[]]]]]]}}" bind:__l="__l"></uinput></view></view></view><view class="button"><button type="default" data-event-opts="{{[['tap',[['order',['$event']]]]]}}" bindtap="__e">提交订单</button></view></view>